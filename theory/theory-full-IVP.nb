Cell[BoxData[{
 RowBox[{
  RowBox[{"coeffsols", " ", "=", " ", 
   RowBox[{"Solve", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"Ann", "*", 
