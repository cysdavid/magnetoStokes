Cell[BoxData[
 RowBox[{
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"BesselY", "[", 
      RowBox[{"1", ",", 
       RowBox[{"\[Mu]", " ", "\[Chi]"}]}], "]"}], "\[Equal]", "0"}], "&&", 
    RowBox[{
     RowBox[{"BesselY", "[", 
      RowBox[{"1", ",", "\[Mu]"}], "]"}], "\[Equal]", "0"}]}], ")"}], "||", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"BesselY", "[", 
      RowBox[{"1", ",", 
       RowBox[{"\[Mu]", " ", "\[Chi]"}]}], "]"}], "\[NotEqual]", "0"}], "&&", 
    
    RowBox[{
     RowBox[{"BesselJ", "[", 
      RowBox[{"1", ",", "\[Mu]"}], "]"}], "\[Equal]", 
     FractionBox[
      RowBox[{
       RowBox[{"BesselJ", "[", 
        RowBox[{"1", ",", 
         RowBox[{"\[Mu]", " ", "\[Chi]"}]}], "]"}], " ", 
       RowBox[{"BesselY", "[", 
        RowBox[{"1", ",", "\[Mu]"}], "]"}]}], 
      RowBox[{"BesselY", "[", 
       RowBox[{"1", ",", 
        RowBox[{"\[Mu]", " ", "\[Chi]"}]}], "]"}]]}]}], ")"}], "||", 
  RowBox[{"(", 
   RowBox[{
    RowBox[{
     RowBox[{"BesselY", "[", 
      RowBox[{"1", ",", 
       RowBox[{"\[Mu]", " ", "\[Chi]"}]}], "]"}], "\[Equal]", "0"}], "&&", 
    RowBox[{
     RowBox[{"BesselY", "[", 
      RowBox[{"1", ",", "\[Mu]"}], "]"}], "\[NotEqual]", "0"}], "&&", 
    RowBox[{
     RowBox[{"BesselJ", "[", 
      RowBox[{"1", ",", 
       RowBox[{"\[Mu]", " ", "\[Chi]"}]}], "]"}], "\[Equal]", "0"}]}], 
   ")"}]}]], "Output",
