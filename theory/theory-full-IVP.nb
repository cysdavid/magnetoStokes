  RowBox[{"coeffSoln2", " ", "=", " ", 
   RowBox[{
    RowBox[{"Solve", "[", 
     RowBox[{
      RowBox[{"coeffsystem", " ", "[", 
       RowBox[{"[", "2", "]"}], "]"}], ",", 
      TemplateBox[{"2"},
       "C"]}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"coeffRatio", " ", "=", " ", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      TemplateBox[{"2"},
       "C"], "/", 
      TemplateBox[{"1"},
       "C"]}], ")"}], "//.", "coeffSoln2"}]}], " "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"detMatrix", "[", "\[Chi]", "]"}], "==", 
  RowBox[{
   RowBox[{"AddSides", "[", 
    RowBox[{
     RowBox[{"Assuming", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"BesselY", "[", 
          RowBox[{"1", ",", "\[Mu]"}], "]"}], "/", " ", 
         TemplateBox[{"1"},
          "C"]}], "\[NotEqual]", "0"}], ",", 
       RowBox[{"Simplify", "[", 
        RowBox[{"MultiplySides", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"coeffsystem", " ", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "//.", "coeffSoln2"}], " ", ",", 
          RowBox[{
           RowBox[{"BesselY", "[", 
            RowBox[{"1", ",", "\[Mu]"}], "]"}], "/", " ", 
           TemplateBox[{"1"},
            "C"]}]}], "]"}], "]"}]}], "]"}], ",", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"BesselJ", "[", 
        RowBox[{"1", ",", "\[Mu]"}], "]"}]}], " ", 
      RowBox[{"BesselY", "[", 
       RowBox[{"1", ",", 
        RowBox[{"\[Mu]", " ", "\[Chi]"}]}], "]"}]}]}], "]"}], "[", 
   RowBox[{"[", "1", "]"}], "]"}]}]}], "Input",
