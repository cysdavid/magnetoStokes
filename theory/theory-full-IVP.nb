          RowBox[{"0", ",", "1", ",", 
           RowBox[{"\[ImaginaryI]", " ", 
            RowBox[{"kn", "/", "\[Gamma]"}]}]}], "]"}]}]}], "==", "0"}], "&&",
       " ", 
      RowBox[{
       RowBox[{
        RowBox[{"Ann", "*", 
         RowBox[{"BesselI", "[", 
          RowBox[{"1", ",", 
           RowBox[{"\[Chi]", " ", 
            RowBox[{"kn", "/", "\[Gamma]"}]}]}], "]"}]}], "+", 
