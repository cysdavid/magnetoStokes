Cell[CellGroupData[{

Cell["\<\
Apply radial boundary conditions to solve for coefficients and obtain full \
solution\
\>", "Subsubsection",
 CellChangeTimes->{{3.855352323775662*^9, 
  3.855352347154537*^9}},ExpressionUUID->"2651713f-b778-45be-bf52-\
1b580e8afa82"],

Cell[TextData[{
 "Now apply the radial boundary conditions: ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          OverscriptBox[
           StyleBox["u", "TI"], "^"], 
          StyleBox["n", "TI"]], 
         RowBox[{"(", "\[Chi]", ")"}], "\[LongEqual]", 
         SubscriptBox[
          OverscriptBox[
           StyleBox["u", "TI"], "^"], 
          StyleBox["n", "TI"]], 
         RowBox[{"(", "1", ")"}], "\[LongEqual]", "0  ", "\[Rule]  ", 
         SubscriptBox[
          OverscriptBox[
           StyleBox["U", "TI"], "^"], 
          StyleBox["n", "TI"]], 
         RowBox[{"(", 
           RowBox[{"\[Chi]", 
             SubscriptBox[
              StyleBox["k", "TI"], 
              StyleBox["n", "TI"]], "/", "\[Gamma]"}], ")"}], "\[LongEqual]", 
         
         SubscriptBox[
          OverscriptBox[
           StyleBox["U", "TI"], "^"], 
          StyleBox["n", "TI"]], 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              StyleBox["k", "TI"], 
              StyleBox["n", "TI"]], "/", "\[Gamma]"}], ")"}], "\[LongEqual]", 
         "0"}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\hat{u}_n (\\chi) = \\hat{u}_n (1) = 0 \\quad\\rightarrow\\quad \
\\hat{U}_n(\\chi k_n/\\gamma) = \\hat{U}_n(k_n/\\gamma) = 0", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "1d29d4d0-a668-4a9f-a2e7-f4c05a93f2c1"],
 ". This yields the system of equations ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"0", "\[LongEqual]", 
         SubscriptBox[
          StyleBox["A", "TI"], 
          StyleBox["n", "TI"]], 
         SubscriptBox[
          StyleBox["I", "TI"], "1"], 
         RowBox[{"(", 
           RowBox[{"\[Chi]", 
             SubscriptBox[
              StyleBox["k", "TI"], 
              StyleBox["n", "TI"]], "/", "\[Gamma]"}], ")"}], "+", 
         SubscriptBox[
          StyleBox["B", "TI"], 
          StyleBox["n", "TI"]], 
         SubscriptBox[
          StyleBox["K", "TI"], "1"], 
         RowBox[{"(", 
           RowBox[{"\[Chi]", 
             SubscriptBox[
              StyleBox["k", "TI"], 
              StyleBox["n", "TI"]], "/", "\[Gamma]"}], ")"}], "-", 
         SuperscriptBox[
          RowBox[{"(", 
            RowBox[{"\[Chi]", 
              SubscriptBox[
               StyleBox["k", "TI"], 
               StyleBox["n", "TI"]], "/", "\[Gamma]"}], ")"}], "-1"]}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "0= A_n I_ 1(\\chi k_n/\\gamma) + B_n K_ 1(\\chi k_n/\\gamma) - (\\chi \
k_n/\\gamma)^{-1}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "3065b76f-03d0-43d6-8236-d3c439d28bde"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"0", "\[LongEqual]", 
         SubscriptBox[
          StyleBox["A", "TI"], 
          StyleBox["n", "TI"]], 
         SubscriptBox[
          StyleBox["I", "TI"], "1"], 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              StyleBox["k", "TI"], 
              StyleBox["n", "TI"]], "/", "\[Gamma]"}], ")"}], "+", 
         SubscriptBox[
          StyleBox["B", "TI"], 
          StyleBox["n", "TI"]], 
         SubscriptBox[
          StyleBox["K", "TI"], "1"], 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              StyleBox["k", "TI"], 
              StyleBox["n", "TI"]], "/", "\[Gamma]"}], ")"}], "-", "\[Gamma]",
          "/", 
         SubscriptBox[
          StyleBox["k", "TI"], 
          StyleBox["n", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
     "0= A_n I_ 1(k_n/\\gamma) + B_n K_ 1(k_n/\\gamma) - \\gamma/k_n", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "175933e3-48f7-4af3-adea-93e2cd819f81"],
 ". Solve this for ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox[
        StyleBox["A", "TI"], 
        StyleBox["n", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "A_n", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "0a5bccfa-2bfc-4934-9187-e763e551798b"],
 " and ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox[
        StyleBox["B", "TI"], 
        StyleBox["n", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "B_n", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "377db0c3-179e-4a0c-ba10-7f49d79ee760"],
 "."
}], "Text",
