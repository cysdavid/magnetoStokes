           RowBox[{"\[ImaginaryI]", " ", "\[Chi]", " ", 
            RowBox[{"kn", "/", "\[Gamma]"}]}]}], "]"}]}]}], "==", "0"}]}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"Ann", ",", "Bnn"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"Assumptions", "\[Rule]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"\[Chi]", ">", "0"}], ",", 
        RowBox[{"kn", ">", "0"}], ",", 
        RowBox[{"\[Gamma]", ">", "0"}]}], "}"}]}]}], "\[IndentingNewLine]", 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
